__pixel 5,10,#ff0000; //red pixel

//fill top right corner with green
let width:int = __width;
let height:int = __height;
__pixelr width-10,height-10,width-1,height-1,#00FF00;

__delay 3000; //delay 3 seconds

//set a random pixel in the bottom right corner to blue
__pixel __randi 5, __randi 5, #0000FF;