//print 1-5 in 4 different ways
//then prints 10000 to mark as finished

//standard while loop
let h:int = 0;

while(h<5){
    h=h+1;
    __print h;
}


let i:int = 0;

//no decl or assignment
for(;i<5;){
    i=i+1;
    __print i;
}


//decl
for(let j:integer = 0;j<5;){
    j=j+1;
    __print j;
}


//traditional for loop
for(let k:integer = 1;k<=5;k=k+1){
    __print k;
}



__print 10000;